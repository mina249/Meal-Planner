<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->

    <style name="Theme.MealPlaner" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#e0e0e0</item>
        <!-- Customize your theme here. -->

    </style>
    <style name="Screen">
        <item name="android:background">#e0e0e0</item>
    </style>
    <style name="Card">
        <item name="android:background">#ebebeb</item>
    </style>
    <style name="Text">
        <item name="android:textColor">#000</item>
    </style>
    <style name="Image">
        <item name="android:background">#000</item>
    </style>
    <style name="icon">
        <item name="android:backgroundTint">#000</item>
    </style>

</resources>